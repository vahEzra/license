from flask import Flask, request, jsonify

app = Flask(__name__)

valid_license_keys = ["999-999-999"]  # Add more valid keys as needed

def validate_license(license_key):
  if license_key in valid_license_keys:
    return True
  else:
    return False

@app.route('/validate_license', methods=['POST'])
def validate_license_endpoint():
  license_key = request.json.get('key')
  if license_key:
    is_valid = validate_license(license_key)
    if is_valid:
      return jsonify({'success': True})
    else:
      return jsonify({'success': False, 'message': 'Invalid license key'})
  else:
    return jsonify({'success': False, 'message': 'Missing license key'})

if __name__ == '__main__':
  app.run()
